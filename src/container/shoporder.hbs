<main>
{{#with navigation}}
      <div class='row'>

        
          <div class='col-12'>
            <div class='card mb-5'>
              <div class='card-body'>
                
                  <div class='nav'>

                    {{#each links}}
                      <div class='nav-item'>
                        <a href='{{href}}' class='nav-link'>
                          {{text}}</a>
                      </div>
                    {{/each}}

                  </div>

                {{/with}}
              </div>
            </div>
            <div class='my-5'>
              <h1>{{header}}</h1>

            </div>
          </div>
      </div>

      <div class='row'>
        <div class='col-8'> 
          {{#with contacts_data}}
            
              <h2 class='mb-3'>{{title}}</h2>
                {{#each buttons}}
                    {{#if isPrimary}}
                      <a class='btn btn-warning'>{{text}}</a>
                    {{/if}}
                    {{#if isOutline}}
                      <a
                        class='btn btn-outline-warning'
                      >{{text}}</a>
                    {{/if}}
                {{/each}}
               <div class="row mt-3">
                {{#with form}}
                  {{#with input}}
                    {{#with surname}} 
                          <div class="col-6 mb-3">
                            <label class='form-label'>{{label}}</label>
                            <input class='form-control' placeholder="{{placeholder}}" />
                            <div class='form-text '>
                              {{caption}} </div>
                           </div>
                      {{/with}}
                      {{#with name}}
                          <div class="col-6 mb-3">
                            
                            <label class='form-label'>{{label}}</label>
                            <input  class='form-control' placeholder="{{placeholder}}"></input> 
                            <div class='form-text '>
                              {{caption}} </div>
                              
                          </div>
                      {{/with}} 
                      {{#with phone}}
                          <div class="col-6 mb-3">
                            
                            <label class='form-label'>{{label}}</label>
                            <input class='form-control' placeholder="{{placeholder}}" />
                            <div class='form-text '>
                              {{caption}} </div>
                            </div> 
                        {{/with}} 
                        {{#with email}}
                          <div class="col-6 mb-3">
                            <label class='form-label'>{{label}}</label>
                            <input  class='form-control' placeholder="{{placeholder}}" />
                            <div class='form-text '>
                              {{caption}} </div>
                          </div>
                        {{/with}} 
                      
                  {{/with}}
                  <hr my-5>
                  {{#each switch}}
                  {{#if isActive}}
                   
                     
                      <div class='form-check form-switch'>

                          <label class='form-check-label'>{{text}}</label>
                          
                          <input
                            name='switch'
                            type='checkbox'
                            class='form-check-input' checked  
                          /> 
                          
                          
                        </div>
                        {{else}}
                        <div class='form-check form-switch'>

                          <label class='form-check-label'>{{text}}</label>
                          
                          <input
                            name='switch'
                            type='checkbox'
                            class='form-check-input'   
                          /> 
                          
                          
                        </div>
                        
                    {{/if}}
                    
                    
                  {{/each}}
                {{/with}}
                
                </div>
          <hr class="mt-3 mb-5">
          
               
         {{/with}}   
          {{#with delivery}}
              <h2>{{title}}</h2>
              <div class="row">
                 <div class="card  mb-2">
                      {{#each select}}
                        <div class="card-body">

                            
                            <label class='form-label'>{{label}}</label>
                            <div class="row">
                              <div class='col-6'>
                                <select class='form-select form-select-sm'>
                                  
                                  <option selected>{{placeholder}}</option>
                                  {{#each options}}
                                  <option value="{{value}}">{{text}}</option>
                                  {{/each}}
                                  
                                </select>
                              </div>
                            </div>
                          </div> 
                    
                      {{/each}}   
                              
                  </div>
                   
                  {{#each delivery_type}} 
                  <div class="form-check">
                  <label class="form-check-label"> {{type}}</label>
                  <input type="radio" class="form-check-input"></div>
                  
                     
                  {{/each}}
                    
                        
              </div>
              
                
            <hr class="my-3">
              {{#with payment}}
                  <h2>{{title}}</h2>
                    {{#each list}}
                    <div class="form-check">
                      <label class="form-check-label"> {{type}}</label>
                      <input type="radio" class="form-check-input">
                      <div class='form-text '>
                      {{#if description}}
      
                      {{description}}
                      {{/if}}</div>
                    </div>
                    {{/each}}

              {{/with}} 
            <hr class="my-3">
            {{#with contact}} 
                  <h2>{{title}}</h2>
                  
                  <div class="row">
                    {{#with surname}}
                    <div class="row">
                      <div class="col-6 mb-3">
                                                    
                                                    <label class='form-label'>{{label}}</label>
                                                    <input  class='form-control' placeholder="{{placeholder}}"></input> 
                                                    <div class='form-text '>
                                                      {{caption}} </div>
                                                      
                                            </div>


                    </div>
                      
                    {{/with}}
                    {{#with name}}
                    <div class="row">
                      <div class="col-6 mb-3">
                              
                              <label class='form-label'>{{label}}</label>
                              <input  class='form-control' placeholder="{{placeholder}}"></input> 
                              <div class='form-text '>
                                {{caption}} </div>
                                
                      </div>
                    </div>
                      

                    {{/with}}
                      {{#with phone}}
                      <div class="col-6 mb-3">
                              
                              <label class='form-label'>{{label}}</label>
                              <input  class='form-control' placeholder="{{placeholder}}"></input> 
                              <div class='form-text '>
                                {{caption}} </div>
                                
                      </div>

                    {{/with}}
                    {{#with email}}
                      <div class="col-6 mb-3">
                              
                              <label class='form-label'>{{label}}</label>
                              <input  class='form-control' placeholder="{{placeholder}}"></input> 
                              <div class='form-text '>
                                {{caption}} </div>
                                
                      </div>

                    {{/with}}
                  </div>
                
            {{/with}} 
            <hr class="my-3">
            {{#with coment}}
            <label class="form-label">{{label}}</label>
            <input class="form-control" placeholder="{{placeholder}}">
            {{/with}}  
            <hr class="my-3">   
           
          {{/with}}
          {{#with coupon}}
          <h2 class="mb-2">{{title}} </h2>
          {{#with select}}
            {{#with availabel_coupon}}
                <label class="form-label">{{label}}</label>
                <select class="form-select form-select-lg">
                  <option selected>{{placeholder}}</option>
                  {{#each options}}
                  <option value="{{value}}">{{text}}</option>
                  {{/each}}
                </select>
            {{/with}}
          <div class="mt-3">
            {{#with friend_coupon}}
                <label class="form-label">{{label}}</label>
                <select class="form-select form-select-sm">
                  <option selected>{{placeholder}}</option>
                  {{#each options}}
                  <option value="{{value}}">{{text}}</option>
                  {{/each}}
                </select>
            {{/with}}
          </div>
          
            
          {{/with}}
          <div class="mt-3">
            {{#with add}}
              {{#with friend}}
                  <label class="form-label form-label">{{label}}</label>
                
                 <input  class='form-control form-control-lg' placeholder="{{placeholder}}"></input>
                <div class="form-text">{{caption}}</div>

              {{/with}}
              {{#with colleague}}
                <label class="form-label">
                  {{label}}
                </label>
                <input class="form-control form-control-sm" placeholder={{placeholder}}> </input>
                <div class="form-text">{{caption}}</div>
              {{/with}}
                
            {{/with}}
          </div>
          
          {{/with}}

          <hr class="my-3">
          {{#with quiz}}
           <h2>{{title}}</h2>
           {{#with preference}}
           <p>{{title}}</p>
                      {{#each options}}
                      <input type="radio" class="form-check-input "> 
                      <label class="form-check-label ">{{this}} </label><br>
                     
                      
                       {{/each}}
           {{/with}}
           <div class="mt-3">
            {{#with your_expirience}}
            <label class="form-label">
             {{label}} 
            </label>
             <input
              
              class='form-control '
            />
             {{/with}}
          </div>
          <div class="mt-3">
            {{#with purchase_frequency}}
            <label class="form-label">{{label}}

            </label>
           
            
            <select class='form-select'>
              <option selected>{{placeholder}}</option>
              {{#each options}}
              <option value='{{value}}'>{{text}}</option>
              
              {{/each}}
            </select>
          
          
          {{/with}}
          </div>
          <div class="mt-3 mb-5">
            {{#with factors}}
              <label class="form-check-label">
                {{title}}
              </label>
              {{#each options}}
              <div class='form-check mt-2'>
          <label class='form-check-label' >{{this}}</label>
          <input
            
            type='checkbox'
            class='form-check-input'
          />
             </div>

            {{/each}}
            {{/with}}

          </div>
          {{/with}}
        
        </div> 
       
        
        
        <div class="col-4">
          <div class="card">
            <div class="card-body">
              {{#each action_block}}
                  {{#if isSuccess}}
                  <button class="btn btn-success mb-1 me-4">
                    {{text}}
                  </button> 
                  {{/if}}
                  {{#if isDanger}}
                  <button class="btn btn-danger my-1 me-4">
                    {{text}}
                  </button> 
                  {{/if}}
                  {{#if isInfo}}
                  <button class="btn btn-info mt-1">
                    {{text}}
                  </button> 
                  {{/if}}
              {{/each}}
            </div> 
            
          </div>
          <div class="card">
            <div class="card-body">
              {{#with total}}
              <h3>{{title}}</h3>
                {{#each item_list}}
                
                  {{name}}: {{price}} <br>
                 
                {{/each}}
                <hr>
                {{#each additional_info}}
                    {{text}}: {{value}} <br>
                {{/each}}
                <hr>
                    {{#with total_price}}
                    {{text}}: {{value}}
                    {{/with}}
                  <hr>
                  {{#each button}}
                 
                    
                    {{#if isPrimary}}
                    <button class="btn btn-primary my-2 ">{{text}}</button>
                    {{/if}}
                    {{#if isOutline}}
                    <button class="btn btn-outline-primary">{{text}}</button>
                    {{/if}}
                  {{/each}}

              {{/with}}
                  

            </div>
          </div>
                
                          

                        

        </div>
        <div class="row">
          
        </div>
       
        
      </div>
      <hr class="mt-5">
      <div class="row">
        <div class="col-12">
            <div class="nav nav-tabs">
              {{#each footer}}
              <div class="nav-item">
                {{#each this}}
                  <a href="{{link}}" class="nav-link">{{text}}</a> 
                 {{/each}}
                 <a href='{{link}}' class='nav-link disabled'>
                Disabled</a>
                </div>
                <div class="nav-item">
                 
                </div>
                
              {{/each}}

            </div>
        </div>
      </div>
   
         
            
            
 

</main>