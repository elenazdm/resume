<div>
  <main>
    <div class='card'>
        <div class='card-body'>

    {{#with heading}}
        <div class='card'>
        <div class='card-body'>
        {{#with main}}
          <h1>{{title}}</h1>
          <p class="text-muted">{{text}}</p>
           {{#with button_text}}
           <button class="btn btn-primary">{{solid}}</button>
           <br>
            <br>
            
            <button class="btn btn-outline-primary"> {{outline}}</button>
          
          {{/with}}
        {{/with}}
        </div>
        </div>
        <br>
        
        <div class='card'>
        <div class='card-body'>
          {{#with second}}
           <h2>{{title}}</h2> 
            <p class="text-muted">{{text}}</p>
            
            {{#with button_text}}
             <button class="btn btn-secondary">{{solid}}</button> <br> <br>
             <button class="btn btn-outline-secondary">{{outline}}</button> 
            {{/with}}
          {{/with}}
        </div>
        </div>
        <br>
         <div class='card'>
        <div class='card-body'>
          
          {{#with subblock}}
            <h3>{{title}}</h3>
            <p class="text-muted">{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-dark">{{solid}}</button> <br> <br>
             <button class="btn btn-outline-dark">{{outline}}</button>
              
            {{/with}}
          {{/with}}
           </div>
           </div>
        </div>
      {{/with}}
    </div>
    <br> <br>
     <div class='card'>
        <div class='card-body'>
      
      {{#with nested_display}}
      
        <div class="display-1">{{title}}</div>
        <br>
       <div class='card'>
        <div class='card-body'>
        {{#with children_display}}
          <div class="display-4">{{title}}</div>
          <br>
          <p class="text-muted">{{text}}</p>
                   
          {{#with button}}
            <button class="btn btn-outline-warning">{{text}}</button>
          {{/with}}
          </div>
          </div>
        {{/with}}
        <br>
        
       <h6>{{header_text}}</h6>
    
        {{#with button}}
          <button class="btn btn-info">{{text}}</button>
        {{/with}}
      {{/with}}
    </div>
    </div>
    
    
    <br>
    <div class='card'>
    <div class='card-body'>
      
        {{#with paragraph}}
            <div class="display-1 text-uppercase">{{title}}</div>
            <br>
            <div class="display-6 text-lowercase">{{text}}</div>
            <br>
            {{#with button}}
            
            <button class="btn btn-link">{{url}}</button> 
            <br>
            
            {{/with}}
            <br>
            <div class='card'>
            <div class='card-body'>
            {{#each list_name}}
            <ul><li> 
                <p><a href="#" class="link-underline-light">{{text}} {{url}} </a></p> 
                 </li></ul> 
            
            {{/each}}
            </div>
            </div> 
            
        {{/with}}
    </div>
    </div>
    
  </main>

