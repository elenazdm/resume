<div>
 <main>
    <div class="alert alert-info" role="alert">
  Facebook
</div>
     
    <em>{{> title data="Users:" big=true}}</em>
    <ul>
      {{#each users}}
      <li>ID: {{id}} </li> 
      <li>Name: {{name}} </li> 
      <li>Gender: {{gender}} </li> 
      <li>Age: {{age}}</li> 
        <li><strong>Friends:</strong> 
        <ul> {{#each friends}} <br> 
          <li>ID: {{id}} </li>
          <li>Name: {{name}} </li>
          <li>Gender: {{gender}} </li> 
          <li>Age: {{age}}</li>
            <li> <em>Mutual friends:</em> 
                {{#each mutual_friends}}
                <ul><br> 
                <li>ID: {{id}} </li>
                <li>Name: {{name}} </li> 
                <li>Gender: {{gender}} </li>
                <li>Age: {{age}}</li>
                  {{/each}} 
              </ul>
        </li> 
          {{#if groups}} <li><b>Groups:</b>
            <ul>
              {{#each groups}}
              <li>ID: {{id}} </li>
              <li>Name: {{name}} </li>
              <li>Description: {{description}} </li>
               <li>Members: 
                  {{#each members}}
                  <ul> 
                   <li>ID: {{id}} </li>
                  <li>Name: {{name}} </li>
                  <li>Gender: {{gender}} </li> 
                  <li>Age: {{age}}</li>
                  </ul>
                  {{/each}}</li>
               
              {{/each}}
              </ul>
              </li>
              {{/if}}
          
        <br>
        {{/each}}
        {{#if groups}}
        <li><strong>Groups:</strong> </li>
         {{#each groups}}
         <ul><br> 
                <li>ID: {{id}} </li>
                <li>Name: {{name}} </li> 
                <li>Description: {{description}} </li>
                <br>
                <li><em>Members: </em>
                  <ul>
                    {{#each members}}
                    <li>ID: {{id}} </li>
                    <li>Name: {{name}} </li> 
                    <li>Gender: {{gender}} </li>
                    <li>Age: {{age}}</li> <br>
                    {{/each}}
                  </ul>
                    
                      
                </li>
         {{/each}}   
         {{/if}}   
      <br> <hr>
     
      {{/each}}
    </ul>
    
        
  </main>
 
</div>